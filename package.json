{
  "name": "node-service",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.15.0",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.19"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "dev:debug": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "lint": "eslint ./src --max-warnings 0 --fix",
    "test": "jest --watchAll false",
    "typeorm": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules ./node_modules/typeorm/cli.js -d src/shared/infra/typeorm",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sentry/node": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "aws-sdk": "^2.1206.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.8.5",
    "celebrate": "^15.0.1",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.2",
    "dotenv": "^16.0.2",
    "escape-html": "^1.0.3",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.8",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.15.9",
    "@types/cors": "^2.8.12",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "husky": "^8.0.0",
    "jest": "^29.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "yarn eslint ./src --max-warnings 0 --fix",
      "git add"
    ]
  }
}
